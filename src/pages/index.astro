---
import Layout from "../layouts/Layout.astro";
import Cards from "../components/Cards.astro";

const response = await fetch(
  "https://eldenring.fanapis.com/api/ammos?limit=10000"
)
  .then((resp) => resp.json())
  .catch((err) =>
    console.log(
      "Whoops sth went from when try to fetc data https://eldenring.fanapis.com/api/ammos"
    )
  );
---

<Layout>
  <Cards data={response?.data} />
</Layout>
